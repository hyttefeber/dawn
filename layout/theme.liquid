  </head>

  <body class="gradient" data-instant-intensity="150" data-instant-intensity="mousedown">
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ "accessibility.skip_to_text" | t }}
    </a>
    {% section 'top-bar' %}
    {% section 'announcement-bar' %}
    {% section 'header' %}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
  {% capture stn_app %}{% include 'station-tabs' %}{% endcapture %}{% if stn_app contains 'Station.Apps' %}{{ stn_app }}{% endif %}
    </main>

    {% section 'top-bar' %}
    {% section 'footer' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
    </ul>

    <script>
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
      }

      window.accessibilityStrings = {
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
      }
    </script>

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

<!-- Back in Stock helper snippet -->
{% render 'back-in-stock-helper' %}
<!-- End Back in Stock helper snippet -->

   <!-- Postlaunch hacks /F -->
 <style>

   /*---Bugfixing---*/
.caption-large #BIS_trigger{
 display: none!important;
}
   #BIS_trigger{
    background-color:#355179;
    color:#fff!important;
   }
   .rf-review-store-date{
     display:none;
   }
   .station-tabs .station-tabs-link .rf-review-star{
     color:#F5DF5D!important;
   }
   .station-tabs-link .rf-reviews{
    float: right;
   }

   /*---CLS fixes---*/
   #MainContent{
     min-height: 100%;
     min-width: 100%;
   }
   #shopify-block-51d9ba96-782a-43d0-80b6-67f7d701abed{
    min-height:28px;
   }

   /*---Visual Tweaks for UX---*/
   .free-shipping{
    margin: 10px 0;
   }
    .collection-grid-section .button-label, .card-information__wrapper .card-information__text{
	font-size: 1.8rem;
    color: #355179;
    letter-spacing: 0.4px;
	}
   .grid__item .card-wrapper .product-form__buttons .BIS_trigger{
 	background-color: #355179!important;
	}
   .card-wrapper{
     margin:0 10px;
